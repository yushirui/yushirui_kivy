# 自定义布局
<YushiruiWidget>:
    # 画布
    canvas:
        # 颜色
        Color:
            # r红色，g绿色，b蓝色，a透明度
			rgba: [.1, .5, .5, 1]

		# 线
		Line:
		    # 点
            points: 310,350, 640,380

        # 点
        Point:
            # 点
            points: 300,200, 300,300
            # 点大小
            pointsize: 3
