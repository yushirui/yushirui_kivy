# 自定义布局
<YushiruiWidget>:
    orientation: 'vertical'

    Slider:
        min: 0
        max: 100
    Slider:
        id: slider_move
        orientation: 'vertical'
        min: -100
        max: 100
        step: 10
        value_track: True
        value_track_color: [1, 0, 0, 1]
        value: 25
        pos: 200, 400
