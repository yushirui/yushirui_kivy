# 自定义布局
<YushiruiWidget>:

    # 视图类名
    viewclass: 'Label'

    # 遍历显示数据
    RecycleBoxLayout:

        # 默认大小
        default_size: None, dp(56)

        # 默认大小
        default_size_hint: 1, None

        # 相对高
        size_hint_y: None

        # 高度
        height: self.minimum_height

        # 垂直布局
        orientation: 'vertical'
