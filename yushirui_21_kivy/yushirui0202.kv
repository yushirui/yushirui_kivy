<PosFloat>:

    canvas:
		Color:
			rgba: [1, 1, 1, 1]
		Rectangle:
			size: self.size
			pos: self.pos

    Button:
        text: 'Hello'
        size_hint: .2, .15
        pos_hint: {'center_x': 0.3, 'top': 0.75}

    Button:
        text: 'Pos'
        size_hint: .2, .15
        pos_hint: {'x': 0.6, 'y': .25}
